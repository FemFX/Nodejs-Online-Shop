<% include partials/header.ejs %> <% products.forEach(product => {%>
  <div class="row ">
    <div class="col-sm-6 col-md-4 product">
      <div class="thumbnail">
        <img src="<%= product.image %>" alt="img" class="img-responsive" />
        <div class="caption">
          <h3><%= product.title %></h3>
          <p class="description"><%= product.description %></p>
          <div class="clearfix">
            <div class="price pull-left">$ <%=product.price %></div>
            <a
              href="/cart/<%= product._id %>"
              class="btn btn-success pull-right"
              role="button"
              >Add To Cart</a
            >
            
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%}) %> <% include partials/footer.ejs %>
